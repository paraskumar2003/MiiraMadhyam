<!-- Schemes intro -->
<div class="container-fluid scheme-section">
  <div class="head">
    <h1>Government <span class="highlight">Schemes</span></h1>
    <div class="underline"></div>
  </div>
  <div class="middle flex jcb">
    <% for( var i=0;i<schemesToRender.length;i++){ %>
      <div class="card-custom flex fdc" data-aos="slide-left">
        <div class="card-img">

          <img src="data:image/<%=schemesToRender.img%>">
        </div>
        <div class="card-body">
         <h1 class="card-title"><%= schemesToRender[i].name %></h1>
          <div class="desc">
               <p><%= schemesToRender[i].issueBy %></p>
               <p><%= schemesToRender[i].keyFeature %></p>
          </div>
          <div class="card-links">
            <!-- <a href="">Read More</a> -->
            <a href="/schemes">View More</a>
          </div>
        </div>
      </div>
  <% } %> 
  </div>
  <div style="width:100%;" class="btn-view-div">
    <button class="btn btn-view-more view-more-schemes" data-aos="fade-in">
      <a href="/schemes">View More</a>
    </button>
  </div>
  <div class="parallax-schemes parallax">
  </div>
</div>
<!-- Schemes intro end-->
<!-- Doctor intro -->
<div class="container-fluid doctor-section">
  <div class="head">
    <h1><span class="highlight">Doctors</span></h1>
    <div class="underline"></div>
  </div>
  
  <div class="middle flex jcb">
    <% for( var i=0;i<doctorsToRender.length;i++){ %>     
        <div class="card-custom flex fdc" data-aos="slide-left">
          <div class="card-img">
            <img src="<%= doctorsToRender[i].img  %> ">
          </div>
          <div class="card-body">
            <h1 class="card-title"><%= doctorsToRender[i].name %></h1>
            <div class="desc">
                 <p><%= doctorsToRender[i].expertise %></p>
                 <p><%= doctorsToRender[i].experience %></p>
            </div>
            <div class="card-links">
              <!-- <a href="">Read More</a> -->
              <a href="/doctors">View More</a>
            </div>
          </div>
        </div>
  <% } %> 
  </div>
  <div style="width:100%;" class="btn-view-div">
    <button class="btn btn-view-more view-more-schemes" data-aos="fade-in"    >
      <a href="/doctors">View More</a>
    </button>
  </div>
  <div class="parallax-gallery parallax">
  </div>
</div>
<!-- Doctor intro end -->


<!-- Slider Script -->
<script src="https://unpkg.com/swiper/swiper-bundle.min.js"></script>
    <!-- Initialize Swiper -->
    <script>
        var swiper = new Swiper(".mySwiper", {
          slidesPerView: 3,
          spaceBetween: 30,
          slidesPerGroup: 1,
          loop: true,
          loopFillGroupWithBlank: true,
          pagination: {
            el: ".swiper-pagination",
            clickable: true,
          },
          navigation: {
            nextEl: ".swiper-button-next",
            prevEl: ".swiper-button-prev",
          },
        });
      </script>


<script src="https://unpkg.com/aos@next/dist/aos.js"></script>
<script>
  AOS.init();
</script>

<!-- caraousel functionality -->
<script>
  let carouselInner = document.querySelector('.carousel-inner');
  carouselInner.firstElementChild.classList.add('active'); 
</script>
<!-- carousel functionality end -->






