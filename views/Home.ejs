
<%-include("partials/Header");-%>


<div id="fadein">

  <!-- Caraousel start -->
  <div
    id="carouselExampleSlidesOnly"
    class="carousel slide    fadein"
    data-bs-ride="carousel"
    data-bs-pause="false"
  >

  <input type="number" class="hidden banner-count" value="<%=banners.length%>">
    <div class="carousel-inner">

      <% banners.forEach((item,index)=>{ %>
        <div class="carousel-item">
          <img src="<%=item.img%>" class="d-block w-100" alt="..." />
        </div>

    <%  }) %>
      <div class="carousel-item">
        <img src="../Images/thumbnail1.jpg" class="d-block w-100" alt="..." />
        <div class="banner-text">
               <div class="wrapper">
                  <h1>Find best government <span class="highlight">schemes.</span></h1>
                  <button class="btn" data-aos="fade-in"><a href="/schemes" class="carousel-btn">Find Now</a></button>
               </div>
        </div>
      </div>
      <div class="carousel-item">
        <img src="../Images/thumbnail4.webp" class="d-block w-100" alt="..." />
        <div class="banner-text">
                 <div class="wrapper" data-aos="fade-down">
                  <h1>Find the best <br><span class="highlight">Doctors.</span></h1>
                 <button class="btn"><a href="/doctors" class="carousel-btn">Find Now</a></button>
                 </div>
        </div>
      </div>
    </div>
  </div>
  <!-- Caraousel end -->
</div>



